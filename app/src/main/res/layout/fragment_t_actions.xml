<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.Trasbordo.TActionsFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center_vertical|center">
        <Button
            android:id="@+id/btnLeerQR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:background="@drawable/shape_red_button_border_dark"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:text="LEER QR"/>

        <Button
            android:id="@+id/btnVerViajes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/shape_green_button_border_dark"
            android:textColor="#ffffff"
            android:textStyle="bold"
            android:text="VER VIAJES PARA TRASBORDO"/>
    </LinearLayout>


</FrameLayout>