        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_t_list_viajes" />

    <fragment
        android:id="@+id/TLeerQRFragment"
        android:name="ibao.alertbus.ecosac.views.Trasbordo.TLeerQRFragment"
        android:layout_width="match_parent"
