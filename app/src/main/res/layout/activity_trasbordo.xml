        android:name="ibao.alertbus.ecosac.views.Trasbordo.TActionsFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_t_actions" />

    <fragment
        android:id="@+id/TListViajes"
